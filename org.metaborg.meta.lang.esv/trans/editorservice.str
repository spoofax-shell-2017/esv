module editorservice

imports

  libstratego-lib
  libspoofax/term/origin
  pack
  outline

rules
  
  editor-analyze:
    (ast, path, project-path) -> (ast, [], [], [])
  
  generate-aterm:
    (selected, position, ast, path, project-path) -> (filename, selected)
    where
      filename  := <guarantee-extension(|"aterm")> path
